programa{
    funcao inicio () {
    real  A, B, soma, produto

    escreva ("Digite o valor de A: ")
    leia (A)
   
    escreva ("Digite o valor de B: ")
    leia (B)
   
    soma = A + B
    produto = A * B
   
    escreva ("A soma de A e B é: ", soma, "\n")
    escreva("O produto de A e B é: ", produto)
    }
}